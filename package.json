{
  "name": "ng2-smart-table",
  "version": "1.7.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:demo": "ng build demo --aot --configuration production",
    "build:lib": "ng build ng2-smart-table --configuration production",
    "build:ci": "npm run build:demo && npm run build:lib",
    "docs:gh-pages": "npm run build:demo -- --base-href '/ng2-smart-table/' && ngh --dir ./dist/demo",
    "test": "ng test",
    "release": "npm run build:lib && npm run publish",
    "publish": "npm publish --access=public dist/ng2-smart-table",
    "changelog": "standard-changelog -p angular",
    "lint": "eslint -c .eslintrc.js --ext .ts projects",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.3",
    "@angular/common": "~13.3.3",
    "@angular/compiler": "~13.3.3",
    "@angular/core": "~13.3.3",
    "@angular/forms": "~13.3.3",
    "@angular/platform-browser": "~13.3.3",
    "@angular/platform-browser-dynamic": "~13.3.3",
    "@angular/router": "~13.3.3",
    "highlight.js": "^11.5.1",
    "lodash": "^4.17.21",
    "ng2-completer": "^9.0.1",
    "rxjs": "~7.5.5",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular/cli": "~13.3.3",
    "@angular/compiler-cli": "~13.3.3",
    "@angular/language-service": "~13.3.3",
    "@types/jasmine": "~4.0.2",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "angular-cli-ghpages": "^1.0.0",
    "eslint": "^8.13.0",
    "jasmine-core": "~4.1.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.18",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.3.1",
    "protractor": "~7.0.0",
    "standard-changelog": "^2.0.27",
    "ts-node": "~10.7.0",
    "typescript": "~4.6.3"
  }
}
